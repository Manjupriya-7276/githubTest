 b) 

class Solution:
    def printTriangle(self, N):
        for i in range(N):
            k=ord("A")
            for j in range(i+1):
                print(chr(k),end="")
                k=k+1
            print()
a)   

class Solution:
    def printTriangle(self, N):
        p=1
        for i in range(N):
            for j in range(i+1):
                print(p,end=" ")
                p+=1
            print()
